{
	"name": "ionic-feedback-module",
	"version": "0.0.0-development",
	"description": "Component for collecting user's feedback in an app built with Ionic",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-feedback-module.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"feedback"
	],
	"scripts": {
		"build": "npm run clean && npm run compile && npm run copy-scss",
		"build-windows": "npm run clean-windows && npm run compile && npm run copy-scss-windows",
		"clean": "rm -f dist/*.d.ts && rm -f dist/*.js && rm -f dist/*.map && rm -f dist/*.json && rm -f dist/*.scss && rm -f src/*.ngfactory.ts",
		"clean-windows": "del -f dist\\*.d.ts && del -f dist\\*.js && del -f dist\\*.map && del -f dist\\*.json && del -f dist\\*.scss && del -f src\\*.ngfactory.ts",
		"commit": "git-cz",
		"compile": "tsc -p tsconfig.json && rollup -c rollup.config.js dist/feedback.module.js > dist/feedback.bundle.js && ngc -p tsconfig.aot.json",
		"copy-scss": "cp src/*.scss dist",
		"copy-scss-windows": "copy \"%cd%\\src\\*.scss\" \"%cd%\\dist\" /Y",
		"lint": "tslint --project tsconfig.aot.json --format verbose",
		"pack": "npm run build && npm pack",
		"pack-windows": "npm run build-windows && npm pack",
		"semantic-release": "semantic-release pre && npm publish && semantic-release post",
		"test": "npm run build && npm run lint"
	},
	"files": [
		"dist",
		"src"
	],
	"main": "dist/feedback.bundle.js",
	"module": "dist/index.js",
	"typings": "dist/index.d.ts",
	"peerDependencies": {
		"@angular/animations": "^6.1.6",
		"@angular/core": "^6.1.6",
		"@angular/forms": "^6.1.6",
		"@angular/platform-browser": "^6.1.6",
		"@angular/platform-browser-dynamic": "^6.1.6",
		"@angular/platform-server": "^6.1.6",
		"@angular/router": "^6.1.6",
		"@ionic/angular": "^4.0.0-beta.8",
		"zone.js": "^0.8.26",
		"@ionic-native/app-version": "^5.0.0-beta.17",
		"@ionic-native/core": "^5.0.0-beta.17",
		"@ionic-native/device": "^5.0.0-beta.17",
		"cordova-plugin-app-version": "^0.1.9",
		"cordova-plugin-device": "^2.0.2"
	},
	"dependencies": {
		"rxjs": "^6.3.0"
	},
	"devDependencies": {
		"@angular/animations": "^6.1.6",
		"@angular/cli": "6.1.5",
		"@angular/common": "^6.1.6",
		"@angular/compiler": "^6.1.6",
		"@angular/compiler-cli": "^6.1.6",
		"@angular/core": "^6.1.6",
		"@angular/forms": "^6.1.6",
		"@angular/platform-browser": "^6.1.6",
		"@angular/platform-browser-dynamic": "^6.1.6",
		"@angular/platform-server": "^6.1.6",
		"@angular/router": "^6.1.6",
		"@ionic-native/app-version": "^5.0.0-beta.17",
		"@ionic-native/core": "^5.0.0-beta.17",
		"@ionic-native/device": "^5.0.0-beta.17",
		"@ionic/angular": "^4.0.0-beta.8",
		"cordova-plugin-app-version": "^0.1.9",
		"cordova-plugin-device": "^2.0.2",
		"cz-conventional-changelog": "2.1.0",
		"reflect-metadata": "^0.1.10",
		"requirejs": "2.3.5",
		"rollup": "0.52.1",
		"tslint": "5.8.0",
		"typescript": "^2.9.2",
		"zone.js": "^0.8.26"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
